<div class="card">
    <div class="card-content">
        <span class="card-title">
            <h5>Verkn√ºpfte Charaktere:</h5>
        </span>
        <table class="bordered highlight">
            <thead>
                <th>Name</th>
                <th>Klasse</th>
                <th>Set</th>
            </thead>
            <tbody>
                {% for char in user.getCharacters %}
                    <tr>
                        <td>{{ char.getCharName }}</td>
                        <td>{{ char.getClmClass }}</td>
                        <td>{{ char.getPreferredSet }}</td>
                    </tr>
                {% endfor %}
            </tbody>
        </table>
    </div>
</div>
