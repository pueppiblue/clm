{% extends 'base.html.twig' %}

{% block body %}
    <div class="row">
        <div class="col s12">
            <br>
            <div class="card-panel">
                <h4>Accounts</h4>
            </div>
        </div>
        {% for user in users %}
            <div class="col s6 m4 l2">
                <a class="collection-item" href="{{ path('user_show', {'id': user.id}) }}">
                    <div class="multi-panel card-panel">
                        {{ user.getAccountName }}
                    </div>
                </a>
            </div>
        {% endfor %}
    </div>

{% endblock %}

