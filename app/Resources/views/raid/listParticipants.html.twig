
{% block body %}
    <div class="row">
        <div class="col s12">
            <div class="card">
                <div class="card-content">
                    <div class="card-title col s12">
                        Player-Pool:
                    </div>
                    <ul class="row">
                        {% for i in 1..40 %}
                            <li class="col s4 m2 l1">
                                <div class="multi-panel card-panel z-depth-1" draggable="false">
                                    spieler {{ i }}
                                </div>
                            </li>
                        {% endfor %}
                    </ul>
                </div>
            </div>
        </div>
    </div>
{% endblock %}
